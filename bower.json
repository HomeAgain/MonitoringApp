{
  "name": "y",
  "version": "y",
  "main": "y",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "backbone": "~1.0.0",
    "backbone.marionette": "~1.1.0",
    "underscore": "~1.5.2",
    "bootstrap": "~3.0.0",
    "console-polyfill": "~0.1.0",
    "jquery": "~2.0.3",
    "jquery.cookie": "~1.4.0",
    "moment": "~2.2.1",
    "bootstrap-switch": "*",
    "processingjs": "*",
    "i18next": "~1.7.1",
    "backbone-validation": "~0.9.1"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "underscore": "*",
        "jquery": "*"
      },
      "main": "backbone.js"
    },
    "processingjs": {
      "main": "processing.js"
    },
    "bootstrap": {
      "dependencies": {
        "jquery": "*"
      },
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css",
        "dist/fonts/*"
      ]
    },
    "bootstrap-switch": {
      "dependencies": {
        "jquery": "*"
      },
      "main": [
        "static/js/bootstrap-switch.js",
        "static/stylesheets/bootstrap-switch.css"
      ]
    },
    "underscore": {
      "main": "underscore.js"
    },
    "backbone-validation" : {
        "main" : [ "backbone-validation-amd-min.js", "backbone-validation-min.js"]
    }
  },
  "resolutions": {
    "jquery": "~2.0.3"
  }
}
